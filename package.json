{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^9.4.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "google-auth-library": "^8.7.0",
    "jsonwebtoken": "^9.0.0",
    "ms": "^2.1.3",
    "nestjs-console": "^8.0.0",
    "next": "^13.1.0",
    "openai": "^3.2.1",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "styled-components": "^5.3.8",
    "ts-morph": "^17.0.1",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@next/eslint-plugin-next": "^13.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.18",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/passport-oauth2": "^1.4.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cypress": "^12.2.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fishery": "^2.2.2",
    "graphql-zeus": "^5.2.4",
    "jest": "^28.1.3",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
